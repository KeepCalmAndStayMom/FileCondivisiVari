GET "http://example.com/api/v2/patients/:patient_id/initial_data" //dati iniziali di gravidanza

//esempio dati iniziali di gravidanza
{
    "patient_id": 1,
    "pregnancy_start_date": "YYYY-MM-DD",
    "weight": 60,
    "height": 160,
